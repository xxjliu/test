    519              {
    520                  CLK->CKDIVR &= (uint8_t)(~CLK_CKDIVR_CPUDIV);
    521                  CLK->CKDIVR |= (uint8_t)((uint8_t)CLK_Prescaler & (uint8_t)CLK_CKDIVR_CPUDIV);
    522              }
